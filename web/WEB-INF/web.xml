<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>LoginPageServlet</servlet-name>
        <servlet-class>Servlets.LoginPageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GameServlet</servlet-name>
        <servlet-class>Servlets.GameServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>Servlets.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EnterGameServlet</servlet-name>
        <servlet-class>Servlets.EnterGameServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddGameServlet</servlet-name>
        <servlet-class>Servlets.AddGameServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PlayerSatMineServlet</servlet-name>
        <servlet-class>Servlets.PlayerSatMineServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PlayerMadeMoveServlet</servlet-name>
        <servlet-class>Servlets.PlayerMadeMoveServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteGameServlet</servlet-name>
        <servlet-class>Servlets.DeleteGameServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckForXmlErrorsServlet</servlet-name>
        <servlet-class>Servlets.CheckForXmlErrorsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IsPlayerOwnGameServlet</servlet-name>
        <servlet-class>Servlets.IsPlayerOwnGameServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>IsPlayerOwnGameServlet</servlet-name>
        <url-pattern>/IsPlayerOwnGameServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckForXmlErrorsServlet</servlet-name>
        <url-pattern>/CheckForXmlErrorsServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteGameServlet</servlet-name>
        <url-pattern>/DeleteGameServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PlayerMadeMoveServlet</servlet-name>
        <url-pattern>/PlayerMadeMoveServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PlayerSatMineServlet</servlet-name>
        <url-pattern>/PlayerSatMineServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EnterGameServlet</servlet-name>
        <url-pattern>/EnterGameServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddGameServlet</servlet-name>
        <url-pattern>/AddGameServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GameServlet</servlet-name>
        <url-pattern>/GameServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginPageServlet</servlet-name>
        <url-pattern>/LoginPageServlet</url-pattern>
    </servlet-mapping>
</web-app>